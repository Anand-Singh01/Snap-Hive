generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name String @default("Akash")
  email    String @unique
  password String
  username String
  imageUrl String @default("https://res.cloudinary.com/dpm7rnqq8/image/upload/v1720614505/snap-Hive/qzw66oqcdbbwfpczbtyk.webp")
}
